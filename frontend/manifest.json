{
  "manifest_version": 3,
  "name": "ByteCode",
  "version": "1.1.0",
  "description": "A phishing detector plugin using AI/Ml",
  "icons": {
   
    "128": "icons/logo.png"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "webNavigation"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "tabs"
],
  "action": {
    "default_popup": "plugin_ui.html",
    "default_icon": {
     
      "128": "icons/logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery.js"
      ]
    }
  ]
}